ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name reason-react
src_root_dir = /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/reason-react
bsc = /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/lib/ocaml -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
refmt = /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_deps_from_reason_intf
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx} ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -intf ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/reactRe.mliast : build_ast_and_deps_from_reason_intf $src_root_dir/src/reactRe.rei
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/reactRe.mliast.d : build_deps src/reactRe.mliast
rule build_cmi
  command = ${bsc} ${bs_package_flags} -bs-no-builtin-ppx-mli -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${out} -c  ${in}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/reactRe.cmi : build_cmi src/reactRe.mliast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/reactRe.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/reactRe.re
build src/reactRe.mlast.d : build_deps src/reactRe.mlast
rule build_cmj_only
  command = ${bsc} ${bs_package_flags} -bs-assume-has-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include  ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/reactRe.cmj $src_root_dir/lib/js/src/reactRe.js : build_cmj_only src/reactRe.mlast |  src/reactRe.cmi
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/reactDOMRe.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/reactDOMRe.re
build src/reactDOMRe.mlast.d : build_deps src/reactDOMRe.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/reactDOMRe.cmj $src_root_dir/lib/js/src/reactDOMRe.js src/reactDOMRe.cmi : build_cmj_cmi src/reactDOMRe.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/reactEventRe.mliast : build_ast_and_deps_from_reason_intf $src_root_dir/src/reactEventRe.rei
build src/reactEventRe.mliast.d : build_deps src/reactEventRe.mliast
build src/reactEventRe.cmi : build_cmi src/reactEventRe.mliast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/reactEventRe.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/reactEventRe.re
build src/reactEventRe.mlast.d : build_deps src/reactEventRe.mlast
build src/reactEventRe.cmj $src_root_dir/lib/js/src/reactEventRe.js : build_cmj_only src/reactEventRe.mlast |  src/reactEventRe.cmi
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/reactDOMServerRe.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/reactDOMServerRe.re
build src/reactDOMServerRe.mlast.d : build_deps src/reactDOMServerRe.mlast
build src/reactDOMServerRe.cmj $src_root_dir/lib/js/src/reactDOMServerRe.js src/reactDOMServerRe.cmi : build_cmj_cmi src/reactDOMServerRe.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/reactDOMServerRe.mlast.d src/reactEventRe.mlast.d src/reactEventRe.mliast.d src/reactDOMRe.mlast.d src/reactRe.mlast.d src/reactRe.mliast.d
