ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name reasonml-react-playground
src_root_dir = /Users/franciscosales/Development/research/react/examples/reasonml-example
bsc = /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/lib/ocaml -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = -I /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/reason-react/lib/ocaml -I /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-director/lib/ocaml
bs_package_dev_includes = 
refmt = /Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = -ppx '/Users/franciscosales/Development/research/react/examples/reasonml-example/node_modules/bs-platform/bin/reactjs_jsx_ppx.exe'
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I src/root -I src
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/root/page.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/root/page.re
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/root/page.mlast.d : build_deps src/root/page.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/root/page.cmj $src_root_dir/lib/js/src/root/page.js src/root/page.cmi : build_cmj_cmi src/root/page.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src/root
build src/root/root.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/root/root.re
build src/root/root.mlast.d : build_deps src/root/root.mlast
build src/root/root.cmj $src_root_dir/lib/js/src/root/root.js src/root/root.cmi : build_cmj_cmi src/root/root.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src/root
build build.ninja : phony  ||  src/root/root.mlast.d src/root/page.mlast.d
